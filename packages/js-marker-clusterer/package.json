{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@agm/js-marker-clusterer",
  "description": "Angular Google Maps (AGM) extension for js-marker-clusterer support",
  "author": "Jens Fahnenbruck <jigfox@me.com>",
  "keywords": [
    "agm",
    "angular",
    "angular-google-maps",
    "google-maps",
    "js-marker-clusterer",
    "markerclustererplus"
  ],
  "peerDependencies": {
    "@angular/core": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@agm/core": "^1.0.0-beta.7",
    "@google/markerclustererplus": "^2.1.11"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "./index.ts",
      "umdId": "ngmaps.jsMarkerClusterer",
      "umdModuleIds": {
        "@agm/core": "ngmaps.core",
        "@google/markerclustererplus": "MarkerClusterer"
      }
    },
    "dest": "../../dist/@agm/js-marker-clusterer"
  }
}
