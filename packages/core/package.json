{
  "name": "@agm/core",
  "version": "3.0.0-beta.1",
  "description": "Angular components for Google Maps",
  "author": "Sebastian Holstein <info@sebastian-holstein.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianM/angular-google-maps.git"
  },
  "bugs": {
    "url": "https://github.com/SebastianM/angular-google-maps/issues"
  },
  "homepage": "https://angular-maps.com",
  "keywords": [
    "angular",
    "ng",
    "google-maps",
    "maps",
    "google",
    "gmaps"
  ],
  "scripts": {
    "build": "./node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cpy --parents schematics/*/schema.json ../../dist/core/",
    "copy:files": "cpy --parents schematics/*/files/** ../../dist/core/",
    "copy:collection": "cpy schematics/collection.json ../../dist/core/schematics/",
    "postbuild": "yarn copy:schemas && yarn copy:files && yarn copy:collection"
  },
  "peerDependencies": {
    "@angular/common": "^9.1.0 || ^10.0.0",
    "@angular/core": "^9.1.0 || ^10.0.0"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/googlemaps": "^3.39.8",
    "@types/jest": "^26.0.3",
    "common-tags": "^1.8.0",
    "cpy": "^8.1.0",
    "cpy-cli": "^3.1.1",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "typescript": "^3.9.7"
  },
  "schematics": "./schematics/collection.json"
}
